{
  "projectType": "application",
  "generators": {
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "sourceRoot": "apps/ui/src",
  "prefix": "fuss",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/ui",
        "index": "apps/ui/src/index.html",
        "main": "apps/ui/src/main.ts",
        "polyfills": "apps/ui/src/polyfills.ts",
        "tsConfig": "apps/ui/tsconfig.app.json",
        "aot": true,
        "assets": [
          "apps/ui/src/favicon.ico",
          "apps/ui/src/assets",
          "apps/ui/src/firebase-messaging-sw.js",
          "apps/ui/src/manifest.webmanifest"
        ],
        "styles": [
          "./node_modules/@angular/material/prebuilt-themes/pink-bluegrey.css",
          "apps/ui/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/ui/src/environments/environment.ts",
              "with": "apps/ui/src/environments/environment.prod.ts"
            },
            {
              "replace": "apps/ui/src/firebase-messaging-sw.js",
              "with": "apps/ui/src/environments/firebase-messaging-sw.prod.js"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "serviceWorker": true,
          "ngswConfigPath": "apps/ui/ngsw-config.json"
        },
        "test": {
          "fileReplacements": [
            {
              "replace": "apps/ui/src/environments/environment.ts",
              "with": "apps/ui/src/environments/environment.test.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": true,
          "extractCss": true,
          "namedChunks": true,
          "extractLicenses": true,
          "vendorChunk": false,
          "buildOptimizer": true,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "6kb",
              "maximumError": "10kb"
            }
          ],
          "serviceWorker": true,
          "ngswConfigPath": "apps/ui/ngsw-config.json"
        }
      }
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "options": {
        "browserTarget": "ui:build"
      },
      "configurations": {
        "production": {
          "browserTarget": "ui:build:production"
        }
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "ui:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/ui/**/*.ts", "apps/ui/**/*.html"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "options": {
        "jestConfig": "apps/ui/jest.config.ts"
      }
    },
    "deploy": {
      "executor": "@angular/fire:deploy",
      "options": {}
    }
  },
  "tags": []
}
